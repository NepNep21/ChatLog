{
  "schemaVersion": 1,
  "id": "chatlog",
  "version": "${version}",

  "name": "ChatLog",
  "description": "A simple minecraft mod for logging chat to a separate file and optionally a discord webhook",
  "authors": [
    "Nep Nep#3025/NepNep21"
  ],
  "contact": {
    "sources": "https://github.com/NepNep21/ChatLog"
  },

  "license": "MIT",

  "environment": "client",
  "entrypoints": {
    "client": [
      "me.nepnep.chatlog.ChatLog"
    ]
  },

  "mixins": [
    "chatlog.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.11.3",
    "minecraft": "1.17.x",
    "java": ">=16"
  }
}
