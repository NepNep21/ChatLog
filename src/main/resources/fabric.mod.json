{
  "schemaVersion": 1,
  "id": "chatlog",
  "version": "${version}",

  "name": "ChatLog",
  "description": "A simple minecraft mod for logging chat to a separate file and optionally a discord webhook",
  "authors": [
    "NepNep21"
  ],
  "contact": {
    "sources": "https://github.com/Lolicon2110/ChatLog"
  },

  "license": "MIT",

  "environment": "client",
  "entrypoints": {
    "client": [
      "me.nepnep.chatlog.ChatLog"
    ]
  },

  "mixins": [
    "chatlog.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.7.4",
    "minecraft": "1.16.x",
    "cloth-config2": ">=4.11.19"
  }
}
